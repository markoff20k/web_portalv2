	}

	&__option {
		background: transparent;
		display: inline-block;
		height: 58px;
		width: 50%;

		.__selected {
			border-bottom: 2px solid var(--primary-cta-color);
			color: var(--primary-text-color);
		}
	}

	&__option-inner {
		background: transparent;
