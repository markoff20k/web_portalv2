.theme-shape-root {
    position: relative;
    z-index: 2;
    overflow: hidden;

    &::before {
        position: absolute;
        left: -250px;
        top: 250px;
        right: auto;
        bottom: auto;
        z-index: -1;
        width: 500px;
        height: 500px;
        border-radius: 1000px;
        background-image: linear-gradient(45deg, #009991, #18988F);
        opacity: 0.2;
        filter: blur(100px);
        content: "";
    }

    &::after {
        position: absolute;
        z-index: -1;
        width: 500px;
        height: 500px;
        border-radius: 1000px;
        opacity: 0.2;
        filter: blur(100px);
        content: "";
        left: auto;
        top: -250px;
        right: -250px;
        bottom: auto;
        background-image: linear-gradient(45deg, #009991, #18988F);
        
    }
}

.theme-shape-left {
    position: relative;


   
        background-image: linear-gradient(45deg, #4a4a4a, #fff);
        opacity: 0.2;
        filter: blur(100px);
        max-width: 100px;

 
}

.theme-shape-center {

    position: relative;


        background-image: radial-gradient(#4a4a4a, transparent at center);

        
    
}



.theme-shape-pulse {
