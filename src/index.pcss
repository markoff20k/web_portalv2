{/* Modal Component*/}

/* stylelint-disable */
.td-mobile-modal {
	background-color: rgba(0, 0, 0, 0.7);
	position: fixed;
	bottom: 0;
	left: 0;
	right: 0;
	height: 0;
	z-index: 99999;
	overflow: hidden;

	&__header {
		height: 36px;
		background: var(--subheader-background-color);
