  .cr-combined-order-book {
	height: 100%;
	position: relative;
  
	.cr-table {
	  &-background {
		&__row {
		  padding: 22px;
		}
	  }
	}
  
	&--data-loading {
	  background: transparent;
	  display: block;
	}
  
	&__market {
	  display: flex;
	  flex-direction: column;
	  margin-top: 5px;
	  padding-bottom: 5px;
  
	  > span {
		font-size: 12px;
		font-weight: normal;
		line-height: 12px;
		text-align: center;
  
