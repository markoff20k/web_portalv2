		color: var(--system-red);
	}

	&__item {
		color: #848e9c;
		padding-bottom: 10px;
		padding-top: 10px;
    font-size: 12px;

		&:not(:last-child) {
			border-bottom: solid 1px #848e9c;
		}

		& > div {
			&:first-child {
				& > span:last-child {
					direction: rtl;
				}
			}

			& > span {
				display: inline-block;
				overflow: hidden;
				text-overflow: ellipsis;
				white-space: nowrap;
				width: 100% !important;

				&:first-child {
					padding-right: 1rem;
				}

				&:last-child {
					display: inline-block;
					padding-left: 1rem;
					text-align: right;
				}
			}
		}

		&__field--highlight {
			color: #fff;
			font-size: 11px;
		}
	}
}

.td-pg-profile__active-step {
	background-color: transparent;
	margin-bottom: 1em;
	margin-left: 1em;
	margin-top: 1.5em;
	overflow: hidden;
	padding: 20px;
	position: relative;
  	opacity: 0.84;
  	height: 30vh;
  	/* width: 100%; */
	width: 1170px !important;


  
    border: 1px solid var(--wallets-item-border-color);
