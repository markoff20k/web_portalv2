					}
				}
			}
		}
	}

	&--no-data-second {
		.cr-order-book {
			&:last-child {
				.cr-table {
					&__body {
						tr {
							td:nth-child(2) {
								color: var(--secondary-contrast-cta-color);
								left: 0;
