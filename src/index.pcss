:root,
.light-mode {
  /*  ================================
      1. General area
    ================================ */
  --main-background-color: rgb(var(--rgb-main-background-color));
  --body-background-color: rgb(var(--rgb-body-background-color));
  --header-background-color: rgb(var(--rgb-header-background-color));
  --subheader-background-color: rgb(var(--rgb-subheader-background-color));
  --dropdown-background-color: rgb(var(--rgb-dropdown-background-color));
  --icons: rgb(var(--rgb-icons));
  --primary-cta-color: rgb(var(--rgb-primary-cta-color));
  --secondary-contrast-cta-color: rgb(var(--rgb-secondary-contrast-cta-color));
  --cta-layer-color: rgb(var(--rgb-cta-layer-color));
  --primary-text-color: rgb(var(--rgb-primary-text-color));
  --text-contrast-color: rgb(var(--rgb-text-contrast-color));
  --asks: rgb(var(--rgb-asks));
  --bids: rgb(var(--rgb-bids));
  --system-yellow: rgb(var(--rgb-system-yellow));
  --system-red: rgb(var(--rgb-system-red));
  --system-green: rgb(var(--rgb-system-green));
  --input-background-color: rgb(var(--rgb-body-background-color));
  --landing-background-color: rgb(var(--rgb-landing-background-color));
  --strength-meter-very-strong: rgb(var(--rgb-strength-meter-very-strong));

  /* =================================
      2. Body background color opacity area
    ================================ */
  --body-background-color-level-1: rgba(var(--rgb-body-background-color), var(--alpha-level-1));
  --body-background-color-level-2: rgba(var(--rgb-body-background-color), var(--alpha-level-2));
  --body-background-color-level-3: rgba(var(--rgb-body-background-color), var(--alpha-level-3));
  --body-background-color-level-4: rgba(var(--rgb-body-background-color), var(--alpha-level-4));
  --body-background-color-level-5: rgba(var(--rgb-body-background-color), var(--alpha-level-5));
  --body-background-color-level-6: rgba(var(--rgb-body-background-color), var(--alpha-level-6));
  --body-background-color-level-7: rgba(var(--rgb-body-background-color), var(--alpha-level-7));
  --body-background-color-level-8: rgba(var(--rgb-body-background-color), var(--alpha-level-8));

  /* =================================
      3. Subheader background color opacity area
    ================================ */
  --subheader-background-color-level-1: rgba(var(--rgb-subheader-background-color), var(--alpha-level-1));
  --subheader-background-color-level-2: rgba(var(--rgb-subheader-background-color), var(--alpha-level-2));
  --subheader-background-color-level-3: rgba(var(--rgb-subheader-background-color), var(--alpha-level-3));
  --subheader-background-color-level-4: rgba(var(--rgb-subheader-background-color), var(--alpha-level-4));
  --subheader-background-color-level-5: rgba(var(--rgb-subheader-background-color), var(--alpha-level-5));
  --subheader-background-color-level-6: rgba(var(--rgb-subheader-background-color), var(--alpha-level-6));
  --subheader-background-color-level-7: rgba(var(--rgb-subheader-background-color), var(--alpha-level-7));
  --subheader-background-color-level-8: rgba(var(--rgb-subheader-background-color), var(--alpha-level-8));

  /* =================================
      4. Dropdown background color opacity area
    ================================ */
  --dropdown-background-color-level-1: rgba(var(--rgb-dropdown-background-color), var(--alpha-level-1));
  --dropdown-background-color-level-2: rgba(var(--rgb-dropdown-background-color), var(--alpha-level-2));
  --dropdown-background-color-level-3: rgba(var(--rgb-dropdown-background-color), var(--alpha-level-3));
  --dropdown-background-color-level-4: rgba(var(--rgb-dropdown-background-color), var(--alpha-level-4));
  --dropdown-background-color-level-5: rgba(var(--rgb-dropdown-background-color), var(--alpha-level-5));
  --dropdown-background-color-level-6: rgba(var(--rgb-dropdown-background-color), var(--alpha-level-6));
  --dropdown-background-color-level-7: rgba(var(--rgb-dropdown-background-color), var(--alpha-level-7));
  --dropdown-background-color-level-8: rgba(var(--rgb-dropdown-background-color), var(--alpha-level-8));

  /* =================================
      5. Icons background color opacity area
    ================================ */
  --icons-level-1: rgba(var(--rgb-icons), var(--alpha-level-1));
  --icons-level-2: rgba(var(--rgb-icons), var(--alpha-level-2));
  --icons-level-3: rgba(var(--rgb-icons), var(--alpha-level-3));
  --icons-level-4: rgba(var(--rgb-icons), var(--alpha-level-4));
  --icons-level-5: rgba(var(--rgb-icons), var(--alpha-level-5));
  --icons-level-6: rgba(var(--rgb-icons), var(--alpha-level-6));
  --icons-level-7: rgba(var(--rgb-icons), var(--alpha-level-7));
  --icons-level-8: rgba(var(--rgb-icons), var(--alpha-level-8));

  /*  ================================
      6. Primary call to action(CTA) color opacity area
    ================================ */
  --primary-cta-color-level-1: rgba(var(--rgb-primary-cta-color), var(--alpha-level-1));
  --primary-cta-color-level-2: rgba(var(--rgb-primary-cta-color), var(--alpha-level-2));
  --primary-cta-color-level-3: rgba(var(--rgb-primary-cta-color), var(--alpha-level-3));
