  }
  


.btn-primary {
    background: var(--button-primary-cta-background-color);
    border: none;
    color: var(--button-primary-cta-text-color);
    font-weight: var(--button-font-weight);
    transition: 0.2s;
  
    &:hover {
      background-color: var(--button-primary-cta-background-color-hover);
      color: var(--button-primary-cta-text-color-hover);
      transition: 0.2s;
    }
  
    &:active,
    &:focus {
      background: var(--button-primary-cta-background-color-active);
      border: none;
      box-shadow: none;
      color: var(--button-primary-cta-text-color-active);
    }
  
    &:disabled {
      background: var(--button-primary-cta-background-color-disabled);
      border: none;
      color: var(--button-primary-cta-text-color-disabled);
      cursor: not-allowed;
    }
  }
  
  .btn-primary:not(:disabled):not(.disabled):active:focus {
    background: var(--button-primary-cta-background-color-active);
    box-shadow: none;
    color: var(--button-primary-cta-text-color-active);
  }
  
  .btn-success {
    background: var(--button-bids-background-color);
    border: none;
    color: var(--button-bids-text-color);
  
    &:hover {
      background: var(--button-bids-background-color-hover);
      color: var(--button-bids-text-color-hover);
      transition: 0.2s;
    }
  
    &:active,
    &:focus {
      background: var(--button-bids-background-color-active);
      border: none;
      box-shadow: none;
      color: var(--button-bids-text-color-active);
    }
  
    &:disabled {
      background: var(--button-bids-background-color-disabled);
      border: none;
      color: var(--button-bids-text-color-disabled);
      cursor: not-allowed;
    }
  }
  
  .btn-success:not(:disabled):not(.disabled):active:focus {
    background: var(--button-bids-background-color-active);
    border: none;
    box-shadow: none;
    color: var(--button-bids-text-color-active);
  }
  
  .btn-danger {
    background: var(--button-asks-background-color);
    border: none;
    color: var(--button-asks-text-color);
  
    &:hover {
      background: var(--button-asks-background-color-hover);
      color: var(--button-asks-text-color-hover);
      transition: 0.2s;
    }
  
    &:active,
    &:focus {
      background: var(--button-asks-background-color-active);
      border: none;
      box-shadow: none;
      color: var(--button-asks-text-color-active);
    }
  
    &:disabled {
      background: var(--button-asks-background-color-disabled);
      border: none;
      color: var(--button-asks-text-color-disabled);
      cursor: not-allowed;
    }
  }
  
  .btn-danger:not(:disabled):not(.disabled):active:focus {
    background: var(--button-asks-background-color-active);
    border: none;
    box-shadow: none;
    color: var(--button-asks-text-color-active);
  }
  
