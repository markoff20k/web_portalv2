    ================================ */

  /* bids button */
  --button-bids-background-color: var(--bids-level-7);
  --button-bids-text-color: var(--primary-text-color-level-7);
  --button-bids-background-color-hover: var(--bids);
  --button-bids-text-color-hover: var(--primary-text-color);
  --button-bids-background-color-active: var(--bids);
  --button-bids-text-color-active: var(--primary-text-color);
  --button-bids-background-color-disabled: var(--bids-level-3);
  --button-bids-text-color-disabled: var(--primary-text-color-level-7);

  /* asks button */
  --button-asks-background-color: var(--asks-level-7);
  --button-asks-text-color: var(--primary-text-color-level-7);
  --button-asks-background-color-hover: var(--asks);
  --button-asks-text-color-hover: var(--primary-text-color);
  --button-asks-background-color-active: var(--asks);
  --button-asks-text-color-active: var(--primary-text-color);
  --button-asks-background-color-disabled: var(--asks-level-3);
  --button-asks-text-color-disabled: var(--primary-text-color-level-7);

  /* percentage button */
  --button-percentage-background-color: var(--dropdown-background-color-level-3);
  --button-percentage-text-color: var(--primary-text-color-level-5);
  --button-percentage-background-color-hover: var(--dropdown-background-color);
  --button-percentage-text-color-hover: var(--primary-text-color);
  --button-percentage-background-color-active: var(--dropdown-background-color);
  --button-percentage-text-color-active: var(--primary-text-color);

  /* primary cta color (blue) button  */
  --button-primary-cta-background-color: var(--primary-cta-color-level-7);
  --button-primary-cta-text-color: var(--cta-layer-color-level-7);
  --button-primary-cta-background-color-hover: var(--primary-cta-color);
  --button-primary-cta-text-color-hover: var(--cta-layer-color);
  --button-primary-cta-background-color-active: var(--primary-cta-color);
  --button-primary-cta-text-color-active: var(--cta-layer-color);
  --button-primary-cta-background-color-disabled: var(--primary-cta-color-level-3);
  --button-primary-cta-text-color-disabled: var(--cta-layer-color-level-7);

  /*  ================================
      20. Header area
    ================================ */
  --header-market-selector-border-color: var(--secondary-contrast-cta-color-level-3);
  --header-market-selector-text-color: var(--primary-text-color);
  --header-toolbar-item-text-color: var(--secondary-contrast-cta-color);
  --header-switcher-border-color-theme: var(--secondary-contrast-cta-color);
  --header-switcher-background-color-theme-active: var(--dropdown-background-color);
  --header-positive-text-color: var(--bids);
  --header-negative-text-color: var(--asks);
  --header-toggle-menu-icon: var(--primary-text-color);

  /*  ================================
      21. Sidebar area
    ================================ */
  --sidebar-background-color: var(--body-background-color);
  --sidebar-text-color: var(--secondary-contrast-cta-color);
  --sidebar-border-color: var(--divider-color-level-1);
  --sidebar-item-background-color-hover: var(--dropdown-background-color-level-5);
  --sidebar-item-text-color-hover: var(--primary-text-color);
  --sidebar-item-background-color-active: var(--dropdown-background-color-level-5);
  --sidebar-item-text-color-active: var(--primary-text-color);

  /*  ================================
      22. Dropdown area
    ================================== */
  --dropdown-text-color: var(--primary-text-color-level-6);
  --dropdown-background-color-hover: var(--secondary-contrast-cta-color-level-1);
  --dropdown-text-color-hover: var(--primary-text-color);
  --dropdown-background-color-active: var(--primary-cta-color);
  --dropdown-text-color-active: var(--primary-text-color);
  --dropdown-scrollbar-button-color: var(--primary-cta-color);
  --dropdown-scrollbar-track-color: var(--secondary-contrast-cta-color-level-1);

  /*  ================================
      23. Input area
    ================================ */
  --input-text-color: var(--primary-text-color);
  --input-text-color-active: var(--primary-cta-color);
  --input-placeholder-color: var(--secondary-contrast-cta-color);
  --input-legend-color: var(--secondary-contrast-cta-color);
  --input-legend-color-active: var(--secondary-contrast-cta-color);
  --input-border-color: var(--secondary-contrast-cta-color);
  --input-border-color-active: var(--primary-cta-color);
  --input-border-color-error: var(--asks);
  --input-checkbox-background-color-active: var(--primary-cta-color);
  --input-checkbox-shadow-color-active: var(--primary-cta-color-level-1);

  /*  ================================
      24. Password strength area
  ================================ */
  --password-strength-meter-background: var(--body-background-color-level-1);
  --password-strength-meter-too-weak: var(--system-red);
  --password-strength-meter-weak: var(--system-yellow);
  --password-strength-meter-good: var(--system-green);
  --password-strength-meter-strong: var(--bids);
  --password-strength-meter-very-strong: var(--strength-meter-very-strong);

  /*  ================================
      25. Footer area
    ================================== */
  --footer-background-color: var(--body-background-color);
  --footer-text-color: var(--secondary-contrast-cta-color);
  --footer-highlight-text-color: var(--primary-text-color);

  /*  ================================
      26. Table area
    ================================ */
  --table-background-color: var(--body-background-color);
  --table-text-color: var(--secondary-contrast-cta-color);
  --table-row-selector-color: var(--dropdown-background-color-level-3);
  --table-header-text-color: var(--secondary-contrast-cta-color);
  --table-border-color: var(--divider-color-level-2);

  /*  ================================
      27. Tab panel area
    ================================ */
  --tab-panel-background-color: var(--subheader-background-color);
  --tab-panel-text-color: var(--secondary-contrast-cta-color);
  --tab-panel-text-color-hover: var(--primary-text-color);
  --tab-panel-text-color-active: var(--primary-text-color);
  --tab-panel-border-color-active: var(--primary-cta-color);
  --tab-panel-border-line-color: var(--divider-color-level-2);

  /*  ================================
      28. Wallets area
    ================================ */
  --wallets-locked-amount: var(--primary-text-color);
  --wallets-locked-amount-active: var(--primary-text-color);
  --wallets-item-font: var(--primary-text-color);
  --wallets-item-font-active: var(--primary-text-color);
  --wallets-item-background-color: var(--body-background-color);
  --wallets-item-background-color-active: var(--dropdown-background-color-level-7);
  --wallets-item-background-color-hover: var(--dropdown-background-color-level-7);
  --wallets-item-border-color: var(--divider-color-level-2);
  --wallets-balance-color: var(--primary-text-color);

  /*  ================================
      29. Beneficiaries area
    ================================ */
  --beneficiaries-primary-0-8: var(--primary-text-color-level-8);
  --beneficiaries-primary-0-1: var(--primary-text-color-level-1);
  --beneficiaries-primary-0-06: rgba(var(--rgb-primary-text-color), calc(var(--alpha) / 2));
  --beneficiaries-primary-0-04: rgba(var(--rgb-primary-text-color), calc(var(--alpha) / 3));

  /*  ================================
      30. Landing page area
    ================================ */
  --light-item-border: rgba(var(--rgb-shadow-color), calc(var(--alpha) / 2));

  /*  ================================
      31. Market selector area
    ================================ */
  --market-selector-background-color: var(--body-background-color);
  --market-selector-header-background-color: var(--subheader-background-color);
  --market-selector-header-text-color: var(--secondary-contrast-cta-color);
  --market-selector-header-text-color-hover: var(--primary-text-color);
  --market-selector-header-text-color-active: var(--primary-text-color);
  --market-selector-header-text-border-active: var(--primary-cta-color);
  --market-selector-table-header-text-color: var(--secondary-contrast-cta-color);
  --market-selector-table-header-text-color-hover: var(--primary-text-color);
  --market-selector-table-text-color: var(--secondary-contrast-cta-color);
  --market-selector-table-text-color-hover: var(--primary-text-color);
  --market-selector-table-text-color-active: var(--primary-text-color);
  --market-selector-table-background-color: var(--body-background-color);
  --market-selector-table-row-selector: var(--secondary-contrast-cta-color-level-1);
  --market-selector-table-row-selector-hover: var(--secondary-contrast-cta-color-level-1);
  --market-selector-input: var(--primary-text-color);
  --market-selector-background-color-input: var(--body-background-color);
  --market-selector-input-border: none;

  /*  ================================
      32. Order book area
    ================================== */
  --order-book-table-text-color: var(--primary-text-color);
  --order-book-last-market-price-subtitle: var(--cta-contrast-level-4);
  --order-book-last-market-price-border-color: var(--divider-color-level-2);
}


:root {
	/*  ALL NAMES FOR VARIABLES TAKE FROM FIGMA
*   IF YOU WANT RENAME VARIABLES, PLEASE FIRST RENAME IT IN FIGMA
*   FOR (100% sync with FIGMA)
*/

	/* --rgb-main-background-color: 23, 28, 41;                switch to light */
	--rgb-main-background-color: 0, 0, 0 !important; /* switch to light */

	/* --rgb-body-background-color: 30, 40, 65;                switch to light */
	--rgb-body-background-color: 0, 0, 0; /* switch to light */

	/* --rgb-header-background-color: 30, 40, 65;             /* switch to light */
	--rgb-header-background-color: 14, 14, 14; /* switch to light */

	/* --rgb-subheader-background-color: 40, 51, 78;           switch to light */
	--rgb-subheader-background-color: 14, 14, 14; /* switch to light */
	--rgb-dropdown-background-color: 14, 14, 14; /* switch to light */
	--rgb-icons: 115, 127, 146; /* switch to light */
	--rgb-primary-cta-color: 60, 120, 224;
	--rgb-contrast-cta-color: 255, 255, 255; /* switch to light */
	--rgb-secondary-contrast-cta-color: 115, 127, 146; /* switch to light */
	--rgb-cta-layer-color: 255, 255, 255;
	--rgb-system-green: 84, 180, 137;
	--rgb-system-red: 232, 94, 89;
	--rgb-system-yellow: 253, 167, 54;
	--rgb-asks: 232, 94, 89;
	--rgb-bids: 84, 180, 137;
	--rgb-primary-text-color: 255, 255, 255; /* switch to light */
	--rgb-text-contrast-color: 23, 28, 41; /* switch to light */
	--rgb-input-background-color: 30, 40, 65; /* switch to light */
	--rgb-divider-color: 255, 255, 255; /* switch to light */
	--rgb-shadow-color: 23, 28, 41; /* switch to light */
	--rgb-landing-background-color: 251, 251, 251;
	--rgb-strength-meter-very-strong: 108, 239, 239;

	/* BUTTON FONT WEIGHT */

	--button-font-weight: 500;

	/*  ALPHA TRANSPERANCY LEVEL */
	--alpha: 0.12;
	--alpha-level-1: var(--alpha);
	--alpha-level-2: calc(var(--alpha) * 2);
	--alpha-level-3: calc(var(--alpha) * 3);
	--alpha-level-4: calc(var(--alpha) * 4);
	--alpha-level-5: calc(var(--alpha) * 5);
	--alpha-level-6: calc(var(--alpha) * 6);
	--alpha-level-7: calc(var(--alpha) * 7);
	--alpha-level-8: calc(var(--alpha) * 8);
}



  
  

  .pg-sign-in-screen {


