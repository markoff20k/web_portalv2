        z-index: -999;
        width: 500px;
        height: 500px;
        border-radius: 1000px;
        background-image: linear-gradient(45deg, #009991, #18988F);
        opacity: 0.1;
        filter: blur(200px);
        content: "";
    }

    &::after {
        position: absolute;
        z-index: -999;
        width: 500px;
        height: 500px;
        border-radius: 1000px;
        opacity: 0.1;
        filter: blur(200px);
        content: "";
        left: auto;
