:root {
    /*  ALL NAMES FOR VARIABLES TAKE FROM FIGMA
  *   IF YOU WANT RENAME VARIABLES, PLEASE FIRST RENAME IT IN FIGMA
  *   FOR (100% sync with FIGMA)
  */
  
    --rgb-main-background-color: 23, 28, 41;               /* switch to light */
    --rgb-body-background-color: 30, 40, 65;               /* switch to light */
    --rgb-header-background-color: 30, 40, 65;             /* switch to light */
    --rgb-subheader-background-color: 40, 51, 78;          /* switch to light */
    --rgb-dropdown-background-color: 61, 68, 98;           /* switch to light */
    --rgb-icons: 115, 127, 146;                            /* switch to light */
    --rgb-primary-cta-color: 60, 120, 224;
    --rgb-contrast-cta-color: 255, 255, 255;               /* switch to light */
    --rgb-secondary-contrast-cta-color: 115, 127, 146;     /* switch to light */
    --rgb-cta-layer-color: 255, 255, 255;
    --rgb-system-green: 84, 180, 137;
    --rgb-system-red: 232, 94, 89;
    --rgb-system-yellow: 232, 175, 89;
    --rgb-asks: 217, 33, 33;
    --rgb-bids: 0, 204, 153;
    --rgb-primary-text-color: 255, 255, 255;               /* switch to light */
    --rgb-text-contrast-color: 23, 28, 41;                 /* switch to light */
    --rgb-input-background-color: 30, 40, 65;              /* switch to light */
    --rgb-divider-color: 255, 255, 255;                    /* switch to light */
    --rgb-shadow-color: 23, 28, 41;                        /* switch to light */
    --rgb-landing-background-color: 251, 251, 251;
    --rgb-strength-meter-very-strong: 108, 239, 239;
  
    /* BUTTON FONT WEIGHT */
  
    --button-font-weight: 500;
  
    /*  ALPHA TRANSPERANCY LEVEL */
    --alpha: 0.12;
    --alpha-level-1: var(--alpha);
    --alpha-level-2: calc(var(--alpha) * 2);
    --alpha-level-3: calc(var(--alpha) * 3);
    --alpha-level-4: calc(var(--alpha) * 4);
    --alpha-level-5: calc(var(--alpha) * 5);
    --alpha-level-6: calc(var(--alpha) * 6);
    --alpha-level-7: calc(var(--alpha) * 7);
    --alpha-level-8: calc(var(--alpha) * 8);
  

  }
  


.btn-primary {
    background: var(--button-primary-cta-background-color);
    border: none;
    color: var(--button-primary-cta-text-color);
    font-weight: var(--button-font-weight);
    transition: 0.2s;
  
    &:hover {
      background-color: var(--button-primary-cta-background-color-hover);
      color: var(--button-primary-cta-text-color-hover);
      transition: 0.2s;
    }
  
    &:active,
    &:focus {
      background: var(--button-primary-cta-background-color-active);
      border: none;
      box-shadow: none;
      color: var(--button-primary-cta-text-color-active);
    }
  
    &:disabled {
      background: var(--button-primary-cta-background-color-disabled);
      border: none;
      color: var(--button-primary-cta-text-color-disabled);
      cursor: not-allowed;
    }
  }
  
  .btn-primary:not(:disabled):not(.disabled):active:focus {
    background: var(--button-primary-cta-background-color-active);
    box-shadow: none;
    color: var(--button-primary-cta-text-color-active);
  }
  
  .btn-success {
    background: var(--button-bids-background-color);
    border: none;
    color: var(--button-bids-text-color);
  
    &:hover {
      background: var(--button-bids-background-color-hover);
      color: var(--button-bids-text-color-hover);
      transition: 0.2s;
    }
  
    &:active,
    &:focus {
      background: var(--button-bids-background-color-active);
      border: none;
