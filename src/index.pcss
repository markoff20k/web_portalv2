		font-weight: 500;
		height: 58px;
		letter-spacing: 0.0357143em;
		line-height: 58px;
		margin: 0 auto;
		text-align: center;
		text-transform: uppercase;
		width: 75%;
	}

	&__form-content {
		bottom: 26px;
		box-sizing: border-box;
		left: 23px;
		position: absolute;
		right: 23px;
		top: 80px;
		background: transparent
	}

	& > * {
		background: transparent;
	}

	&__error-message {
		align-items: center;
		background: transparent;
		color: var(--system-red);
		display: flex;
		flex: 1;
		font-size: calc(var(--gap) * 2.5);
		justify-content: center;
		min-height: calc(var(--gap) * 5);
	}

	&__group {
		background: transparent;
		margin: 20px 0 -10px;
		position: relative;
	}

	&__error {
		background: transparent;
		color: var(--system-red);
		margin-top: 5px;
	}

	&__group--focused {
		.cr-sign-in-form__label {
			color: transparent !important;
			margin-left: -45px !important;
		}
	}

	&__image {
		margin-top: -60px !important;
		height: 100px;
		
		width: 100%;

	}


	&__button-wrapper {
		background: transparent;
		bottom: 55px;
		display: flex;
		flex-direction: column;
		justify-content: space-between;
		left: 0;
		min-height: calc(var(--gap) * 28);
		position: absolute;
		right: 0;



	}

	&__button-wrapper--empty {
		justify-content: flex-end;
	}

	&__bottom-section {
		background: transparent;
		bottom: -10px;
