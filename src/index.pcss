.td-mobile-new-modal {
	&__blur {
		background: rgba(0, 0, 0, 0.6);
		height: 100%;
		left: 0;
		position: fixed;
		top: 0;
		width: 100%;
		z-index: 999;
	}

	&__header {
		font-weight: bold;
		text-align: center;
	}

	&__main {
		background: var(--mobile-body-background-color);
		border-radius: 0.2rem;
		height: auto;
		left: 50%;
		padding: 1rem 0.5rem 2rem 0.5rem;
		position: fixed;
		top: 50%;
		transform: translate(-50%, -50%);
		width: 90%;

		&__body {
			color: #fff;
			margin-top: 2rem;
			text-align: center;

			.green-btn {
				background-color: #009991;
				border: none;
				border-radius: var(--mobile-border-radius-small-size);
				color: #fff;
				font-size: 1rem;
				height: 2rem;
				outline: none;
			}
		}

		&__close-btn {
			background-color: #fff;
			border: none;
			border-radius: 50%;
			height: 1.5rem;
			outline: none;
			position: absolute;
			right: 0;
			top: 0;
			-webkit-transform: translate(40%, -40%);
			transform: translate(40%, -40%);
			width: 1.5rem;

			.sr-only {
				border: 0;
				clip: rect(0, 0, 0, 0);
				height: 1px;
				margin: -1px;
				overflow: hidden;
				padding: 0;
				position: absolute;
				white-space: nowrap;
				width: 1px;
			}
		}
	}

	.display-block {
		display: block;
	}

	.display-none {
		display: none;
	}
}

.td-mobile-referral-content {
	font-size: 14px;

	&__wrapper {
		background: #313445;
		padding: 15px;
	}

	&__table {
		padding: 50px 0;

		&__row {
			&__col-left {
				position: relative;

				h4 {
					background: #313445;
					color: #fff;
					font-size: 18px;
					line-height: 27px;
					margin-bottom: 0;
					padding: 12px;
				}

				&__loading {
					height: 100%;
					padding-top: 3rem;
					position: absolute;
					width: 100%;
					z-index: 20;
				}

				&__friends {
					&-email {
						-webkit-box-orient: vertical;
						display: -webkit-box;
						-webkit-line-clamp: 1;
						overflow: hidden;
					}

					&-time {
						-webkit-box-orient: vertical;
						display: -webkit-box;
						-webkit-line-clamp: 1;
						overflow: hidden;
					}
				}
			}

			&__col-right {
				h4 {
					background: #313445;
					color: #fff;
					font-size: 18px;
					line-height: 27px;
					margin-bottom: 0;
					padding: 12px;
				}

				&__commision {
					&-email {
						-webkit-box-orient: vertical;
						display: -webkit-box;
						-webkit-line-clamp: 2;
						overflow: hidden;
					}
				}
			}
		}

		&-pagination {
			display: flex;
			flex-direction: row;
			justify-content: center;
			margin-top: 10px;
			padding-left: 0;

			li {
				align-items: center;
				background-color: transparent;
				border: none;
				border-radius: 50%;
				color: #fff;
				cursor: pointer;
				display: flex;
				font-size: 14px;
				height: 30px;
				justify-content: center;
				outline: none;
				text-align: center;
				transition: all 0.3s;
				width: 30px;
			}

			.active {
				background-color: #009991 !important;
			}
		}
	}
}

.pg-mobile-user-info {
	align-items: flex-end;
	background: var(--mobile-body-background-color);
	display: flex;
	flex-direction: row;
	height: 87px;
	justify-content: space-between;
	margin: 4px 0;
	padding: 22px 16px;
	width: 100%;

	&__details {
		align-items: flex-start;
		display: flex;
		flex-direction: column;
		height: 100%;
		justify-content: flex-start;
		width: 100%;

		span {
			color: var(--primary-text-color);
			font-size: 16px;
			letter-spacing: 0.1px;
			line-height: 16px;
			margin-bottom: 12px;

			&:last-child {
				font-weight: 500;
				margin-bottom: 0;
			}
		}
	}

	&__logout-button {
		align-items: center;
		border: 1px solid #009991;
		border-radius: 4px;
		color: #009991;
		display: flex;
		font-size: 11px;
		font-weight: normal;
		height: 36px;
		justify-content: center;
		letter-spacing: 0.1px;
		min-width: 98px;
		padding: 0 6px;
		width: max-content;
	}
}


