			grid-column: 1/2;
			grid-row: 2/3;

			div:nth-child(1) {
				text-align: right;
			}

			div:nth-child(2) {
				text-align: left !important;
			}
		}

		.td-mobile-cpn-order-book__header--grid-item-2 {
			grid-column: 2/3;
			grid-row: 2/3;
		}

		.td-mobile-cpn-order-book__combined--grid-item-1 {
			grid-column: 1/2;
			grid-row: 3/4;

			table tbody tr {
				flex-direction: row-reverse;

				.td-mobile-cpn-order-book__combined--bg-ask-row {
					left: unset;
					right: 0;
				}
			}
		}

		.td-mobile-cpn-order-book__combined--grid-item-2 {
			grid-column: 2/3;
			grid-row: 3/4;
		}
	}

	&__header {
		color: #848e9c;
		display: flex;
		font-size: 0.75em;
		padding-bottom: 0.3em;

		&__item {
			flex: 1;
		}
	}

	&__market {
		color: #e84946;
		display: flex;
		flex-direction: column;
		margin: 0.4em;

		&__last-price {
			font-size: 1em;
			font-weight: 500;
		}

		&__convert-usd {
			font-size: 0.75em;
			font-weight: 400;
		}
