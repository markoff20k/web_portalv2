      }
  
      .cr-success-create {
        min-height: calc(var(--big-gap) * 6.46);
  
        .pg-copyable-text__section {
          margin: auto;
          max-width: calc(var(--big-gap) * 6.33);
          overflow: hidden;
          padding: calc(var(--gap) * 4) 0 calc(var(--gap) * 2) 0;
          position: relative;
  
          fieldset {
            border: 1px solid var(--input-border-color);
            border-radius: var(--gap);
            padding: 0 8px 0 9px;
  
            legend {
              color: var(--input-legend-color);
            }
  
            &:focus-within {
              border: 1px solid var(--input-border-color-active) !important;
  
              legend,
              label {
                color: var(--input-legend-color-active) !important;
              }
            }
  
            .cr-copyable-text-field {
              border: none;
              padding: 9px 2px;
  
              .input-group {
                display: flex;
                flex-direction: row;
                justify-content: space-between;
  
                .custom-input {
                  width: 80%;
  
                  label {
                    margin-top: -18px;
                  }
                }
  
                .btn-primary {
                  border-radius: 0.3rem;
                }
              }
            }
          }
        }
      }
    }
  
    &__close {
      cursor: pointer;
      display: inline-block;
      height: calc(var(--gap) * 2.5);
      margin-left: calc(var(--gap) / 1.33);
      overflow: hidden;
      position: relative;
      width: calc(var(--gap) * 2.5);
  
      &:hover {
        &::before,
        &::after {
          background: var(--primary-text-color);
          transition: background 0.2s;
        }
      }
  
      &::before,
      &::after {
        background: var(--secondary-contrast-cta-color);
        content: "";
        height: 2px;
        left: 0;
        position: absolute;
        top: 55%;
        transition: background 0.2s;
        width: 100%;
      }
  
      &::before {
        transform: rotate(45deg);
      }
  
      &::after {
        transform: rotate(-45deg);
      }
    }
  
    &__pull-right {
      color: var(--secondary-contrast-cta-color);
      float: right;
      transition: 0.2s;
  
      &:hover {
        color: var(--primary-text-color);
        transition: 0.2s;
      }
    }
  
    &__text-center {
      padding: 10px;
      text-align: center;
  
      .cr-button {
        padding: 0;
      }
    }
  
    &__text-success {
      color: var(--primary-text-color);
      position: relative;
      width: 100%;
  
      &::after {
        color: var(--primary-text-color);
        content: " ✓";
        display: inline;
        font-size: calc(var(--font-size) * 2.5);
        position: absolute;
        right: 10px;
        top: 0;
      }
    }
  
    .cr-modal__container {
      min-width: 20% !important;
      width: calc(var(--gap) * 69.3);
  
      &-header {
        background-color: var(--email-confirmation-screen-title-background);
        margin-bottom: 0;
      }
  
      &-content {
        padding: var(--email-form-padding) var(--email-form-padding) 0 !important;
  
        fieldset {
          border: 1px solid var(--kyc-grey);
          border-radius: calc(var(--gap) * 0.66);
          box-sizing: border-box;
          color: var(--kyc-grey);
          height: calc(var(--gap) * 8.16);
          margin-top: calc(var(--gap) * 3.6);
          width: 100%;
        }
      }
  
      &-footer {
        padding: 0 var(--email-form-padding) var(--email-form-padding);
      }
    }
  
    &__level {
      margin-left: calc(var(--gap) * 2) !important;
    }
  
    &__vertical-line {
      background: var(--profile-box-header-line);
      height: calc(var(--gap) * 6.4);
      min-height: calc(var(--gap) * 3.2);
      width: calc(var(--gap) * 0.5);
    }
  
    &__btn-secondary {
      color: var(--primary-text-color);
      max-width: calc(var(--gap) * 25);
  
      &:visited {
        color: var(--text-muted);
      }
  
      &__active {
        &:visited {
          color: var(--primary-text-color);
        }
      }
  
      &:active {
        color: var(--primary-text-color);
      }
  
      &:hover {
        background: var(--profile-btn);
        color: var(--white);
      }
  
      &:focus {
        outline: none;
      }
  
      &-change {
        font-size: var(--font-size);
        font-weight: bolder;
        height: calc(var(--gap) * 5);
        width: calc(var(--gap) * 20) !important;
      }
    }
  
    &__currency {
      color: var(--profile-accent-color);
      display: block;
      font-size: 1.5rem !important;
      font-weight: lighter !important;
      margin-top: var(--gap);
    }
  
    &__error {
      color: var(--order-error-color);
      font-size: 1.2rem;
      margin: calc(var(--gap) * 5) 0;
    }
  }
  
  @media (max-width: 780px) {
    .pg-profile-page {
      flex-direction: column;
    }
  }
  
  .cr-modal {
    .cr-email-form {
      background: var(--body-background-color);
      min-height: calc(var(--big-gap) * 5);
      width: calc(var(--big-gap) * 7.06);
  
      &__option {
        background: var(--subheader-background-color);
        border-bottom: 2px solid var(--divider-color-level-2);
      }
  
      &__options-group {
        height: calc(var(--big-gap) * 0.97);
        width: 100%;
      }
  
      &__label {
        background: var(--input-background-color);
        position: absolute;
        top: 0;
      }
  
      &__input {
        input {
          border: 1px solid var(--input-border-color);
          border-radius: calc(var(--gap) * 0.66);
          color: var(--input-text-color);
          font-size: calc(var(--gap) * 2.33);
        }
  
        input[type="number"]::-webkit-inner-spin-button,
        input[type="number"]::-webkit-outer-spin-button {
          -webkit-appearance: none;
          margin: 0;
        }
      }
  
      &__header {
        color: var(--secondary-contrast-cta-color);
        font-size: calc(var(--gap) * 2.33);
        font-style: normal;
        font-weight: normal;
        height: calc(var(--gap) * 4.33);
        line-height: calc(var(--gap) * 2.66);
        margin: calc(var(--gap) * 0.83);
        width: calc(var(--big-gap) * 6.3);
      }
  
      &__group--focused {
        .cr-sign-in-form__label {
          color: var(--input-legend-color-active);
        }
  
        input {
          border-color: var(--input-border-color-active) !important;
        }
      }
  
      &__button-wrapper {
        min-height: calc(var(--gap) * 10);
      }
  
      .pg-profile-page__close {
        cursor: pointer;
        display: inline-block;
        height: calc(var(--gap) * 2.5);
        margin-top: calc(var(--gap) * 2.6);
        overflow: hidden;
        position: absolute;
        right: calc(var(--gap) * 3.33);
        width: calc(var(--gap) * 2.5);
      }
    }
  }
  
  @media (max-width: 767px) {
    .pg-profile-page-header-first {
      display: none;
    }
  
    tr {
      font-size: 0.8em;
      padding-left: 5px;
      padding-right: 5px;
    }
  
    .pg-profile-page__api-keys {
      .cr-table {
        tr,
        td,
        th {
          padding-left: 5px !important;
          padding-right: 5px !important;
        }
      }
    }
  
    .pg-profile-page__api-keys__state-checkbox {
      display: none;
    }
  }
  
  @media (max-width: 460px) {
    .pg-profile-page-header-first {
      display: none;
    }
  
    tr {
      font-size: 0.6em;
      padding-left: 5px;
      padding-right: 5px;
    }
  }
  
  @media (max-width: 360px) {
    .pg-profile-page__text-success::after {
      display: none;
    }
  
    tr,
    td {
      font-size: 0.5em;
    }
  }
  

  .td-pg-profile {
	color: #fff;
	font-size: 12px;

	&--color--second {
		color: #848e9c;
	}

	&--bg {
		background: rgb(49, 52, 69);
		box-shadow: 0 4px 8px -2px rgba(0, 0, 0, 0.15);
	}

	&--bg--second {
		background: rgb(49, 52, 69);
		box-shadow: 0 4px 8px -2px rgba(0, 0, 0, 0.15);
	}

	&--radius {
		border-radius: 10px;
	}

	.container-fluid {
		padding: 0 30px;
	}

	&__quick {
		&__inner {
			padding-bottom: 24px;
			padding-top: 24px;
		}

		&__info--top {
			font-size: 14px;
		}

		&__logo {
			margin-right: 17px;
			min-height: 30px;
			position: relative;

			&__name {
				color: #313445;
				font-size: 20px;
				left: 50%;
				position: absolute;
				text-transform: uppercase;
				top: 50%;
				transform: translate(-50%, -50%);
				z-index: 1;
			}
		}
	}

	&__content {
		display: grid;
		grid-gap: 26px;
		grid-template-columns: 27.5fr 27.5fr 45fr;
		grid-template-rows: repeat(8, calc(720px / 8));
		margin-bottom: 40px;

		&__item {
			display: flex;
			flex-direction: column;
			grid-row: 1/5;
			padding: 17px 30px 27px 27px;

			&:is(&--no-content) {
				padding-bottom: 17px;
				text-decoration: none;

				.td-pg-profile__content__item__header {
					border: none;
					flex: 1;
					height: unset;
				}
			}

			&:is(&--no-content):hover,
			&__header--link:hover {
				text-decoration: none;

				.td-pg-profile__content__item__header__to-page {
					path {
						fill: #fff;
					}
				}
			}

			&__content {
				flex: 1;
				position: relative;
			}

			&__header--link {
				text-decoration: none;
			}

			&__header {
				align-items: center;
				display: flex;
				justify-content: space-between;

				&__title,
				&__desc {
					align-items: center;
					display: flex;
				}

				&__title {
					color: #fff;
					font-size: 16px;
					font-weight: 600;
					height: 100%;
				}

				&__desc {
					color: #848e9c;
					flex: 1;
					font-size: 14px;
					height: 100%;
					padding-left: 25px;
					padding-right: 25px;
				}

				&__to-page {
					svg:hover path {
						fill: #fff;
					}
				}
			}

			.td-tabs {
				display: flex;
				flex-direction: column;
				height: 100%;

				&-content-holder {
					position: relative;
				}
			}

			.td-tabs-nav,
			&__header {
				border-bottom: solid 1px #848e9c;
				height: 52px;
			}

			.td-tabs-nav {
				border-bottom: solid 1px #848e9c;
				height: 52px;

				.td-tabs-tab {
					color: #848e9c;
					font-size: 14px;

					&:not(:last-child) {
						margin-right: 20px;
					}
				}

				.td-tabs-tab-active {
					border-bottom: solid 2px #2fb67e;
					color: unset;
					font-weight: 600;
				}
			}
		}
	}

	&__inner {
		bottom: 0;
		left: 0;
		overflow-y: scroll;
		position: absolute;
		right: 0;
		top: 0;
	}

	&__security {
		grid-row: 1/4;
	}

	&__announcement {
		grid-row: 1/4;

		&__item {
			flex: 1;
			padding: 20px 0;

			&:not(:last-child) {
				border-bottom: solid 1px #848e9c;
			}

			&__title {
				padding-right: 20%;

				a {
					-webkit-box-orient: vertical;
					color: #848e9c;
					display: block;
					display: -webkit-box;
					font-size: 16px;
					height: 45px;
					-webkit-line-clamp: 2;
					overflow: hidden;
					text-decoration: none;
					text-overflow: ellipsis;

					&:hover {
						color: #2fb67e;
					}
				}
			}

			&__date {
				align-items: flex-end;
				display: flex;
				flex: none;
			}
		}
	}

	&__task-center {
		grid-column: 2/4;
		grid-row: 4/5;
	}

	&__list-api-key {
		grid-column: 1/4;
		grid-row: 5/9;
	}

	&__close {
		bottom: 0;
		color: rgb(147, 149, 158);
		cursor: pointer;
		font-size: 25px;
		font-weight: 700;
		line-height: 24px;
		position: absolute;
		right: 0;
		text-shadow: 0 1px 0 rgb(147, 149, 158);
		top: 0;

		&:hover {
			color: var(--primary-text-color);
			text-shadow: 0 1px 0 var(--primary-text-color);
		}
	}

	&__action-delete {
		cursor: pointer;
		display: block;
		font-size: 14px;
		font-weight: 600;
		height: 100%;
		width: 100%;

		&:hover {
			color: #fff;
		}
	}
}

.td-pg-profile__activity {
	&-result-succeed {
		color: var(--system-green);
	}

	&-result-failed {
		color: var(--system-red);
	}

	&__item {
		color: #848e9c;
		padding-bottom: 16px;
		padding-top: 16px;

		&:not(:last-child) {
			border-bottom: solid 1px #848e9c;
		}

		& > div {
			&:first-child {
				& > span:last-child {
					direction: rtl;
				}
			}

			& > span {
				display: inline-block;
				overflow: hidden;
				text-overflow: ellipsis;
				white-space: nowrap;
				width: calc(50%);

				&:first-child {
					padding-right: 1rem;
				}

				&:last-child {
					display: inline-block;
					padding-left: 1rem;
					text-align: right;
				}
			}
		}

		&__field--highlight {
			color: #fff;
			font-size: 14px;
		}
	}
}

.td-pg-profile__active-step {
	background-color: #002145;
	margin-bottom: 40px;
	margin-top: 40px;
	overflow: hidden;
	padding: 20px;
	position: relative;

	&__mask-icon {
		position: absolute;
		right: 0;
		top: 0;
	}

	&__title {
		font-weight: 700px;
	}

	&__content {
		margin-top: 20px;

		&__item {
			position: relative;
			width: 210px;

			&:not(:first-child) {
				::before {
					background-color: #c4c4c4;
					content: '';
					height: 1px;
					position: absolute;
					right: 100%;
					top: calc(21px / 2);
					width: 100%;
				}
			}

			&__step {
				align-items: center;
				background-color: #c4c4c4;
				border-radius: 50%;
				color: #000;
				display: flex;
				height: 21px;
				justify-content: center;
				position: relative;
				width: 21px;
				z-index: 2;
			}

			&__step--active {
				background-color: #ecb22d;
			}

			&__title {
				margin-top: 20px;
			}

			&__desc {
				display: block;
				padding-right: 40px;
			}

			&__action {
				background-color: rgb(47, 182, 126);

				&:hover {
					background-color: rgba(47, 182, 126, 0.8);
				}
			}
		}
	}
}
