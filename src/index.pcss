		&--ask-color {
			color: #e84946;
		}

		&--bid-color {
			color: #2fb67e;
		}

		table {
			width: 100%;

			tbody {
				tr {
					cursor: pointer;
					display: flex;
					justify-content: space-between;
					position: relative;

					td {
						font-size: 0.75em;
						line-height: 1.84em;
						padding-bottom: unset;
						padding-top: unset;

						&:first-child {
							padding-left: 0.4em;
						}

						&:last-child {
							padding-right: 0.4em;
						}

						.td-mobile-cpn-order-book__combined--bg-bid-row,
						.td-mobile-cpn-order-book__combined--bg-ask-row {
							bottom: 0;
							left: 0;
							position: absolute;
							top: 0;
						}

						.td-mobile-cpn-order-book__combined--bg-ask-row {
							background: rgba(232, 73, 70, 0.16);
						}

						.td-mobile-cpn-order-book__combined--bg-bid-row {
							background: rgba(47, 182, 126, 0.15);
