    display: flex;
    flex-direction: row;
    height: 36px;
    justify-content: space-between;
    margin: 0;
    padding: 0 20px;
    width: 100%;

    &__block {
      align-items: center;
      display: flex;
      flex-direction: row;
      height: 32px;
      margin: 0;
      padding: 0;
      width: 100%;

      &:first-child {
        color: var(--primary-text-color);
        font-size: 16px;
        font-weight: bold;
        justify-content: flex-start;
        letter-spacing: 0.15px;
      }

      &:last-child {
        justify-content: flex-end;

        span {
          align-items: center;
          color: var(--primary-cta-color);
          display: flex;
          font-size: 14px;
          font-weight: normal;
          height: 32px;
          letter-spacing: 0.1px;
          padding-right: 8px;
        }

        svg {
          path {
