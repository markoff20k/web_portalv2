	border-radius: 4px;
	display: flex;
	flex-direction: row;
	height: 38px;
	justify-content: space-between;
	width: 100%;
  
	&__fieldset {
	  border-radius: 0;
	  display: flex;
	  width: calc(100% - 72px);
  
	  .custom-input {
		label {
		  font-size: 10px;
		  left: auto;
		  margin-left: 6px;
		  top: auto;
		}
  
		input {
		  align-items: center;
		  border: none;
		  border-radius: 0;
		  display: flex;
		  flex-direction: row;
		  height: 36px;
		  justify-content: flex-start;
		  padding: 0 8px;
		  width: 100%;
		}
