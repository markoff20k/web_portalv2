{/* Current Market Info Component */}

.td-mobile-cpn-current-market-info {
	&__drawer {
		.ant-drawer-body {
			background: var(--mobile-body-background-color);
			padding: 0;
		}

		&__header {
			margin: var(--mobile-main-margin-size);
		}

		.td-mobile-cpn-all-market__body {
			box-shadow: none;
			padding: 0;
		}
	}

	&__header {
		background: var(--mobile-body-background-color);
		border-radius: 3px 3px 0 0;
		box-shadow: 0 4px 8px -2px rgba(0, 0, 0, 0.15);
		font-size: 0.875em;
		line-height: 1.715em;
		padding: 0.6em 0.5em;

		img {
			cursor: pointer;
			margin: 0;
		}

		&__name {
			color: #fff;
		}

		&__percent {
			font-size: 0.714em;

			&--positive {
				color: var(--header-positive-text-color);
			}

			&--negative {
				color: var(--header-negative-text-color);
			}
		}

		&__option {
			position: relative;

			.list-group {
				bottom: 100;
				display: none;
				position: absolute;
				right: 0;
				width: max-content;
				z-index: 1;

				&-item {
					align-items: center;
					background-color: var(--main-background-color);
					color: #fff;
					display: flex;
					padding: 0.2em 0.4em;

					svg {
						margin-right: 0.5em;
					}

					&:hover {
						background-color: #000;
					}
				}
			}
		}
	}

	&__content {
		.pg-trading-chart {
			background: var(--mobile-body-background-color);
			border-radius: 0 0 3px 3px;
			box-shadow: 0 4px 8px -2px rgb(0 0 0 / 15%);
			height: 430px;
			padding: 0.4em 8px 0.875em;
		}
	}

	&__history {
		background: var(--mobile-body-background-color);
		border-radius: var(--mobile-border-radius-small-size);
		box-shadow: 0 4px 8px -2px rgb(0 0 0 / 15%);
		padding: 0.4em 8px 0.875em;

		.td-tabs {
			&-ink-bar {
				background-color: #2fb67e;
				bottom: 0;
				box-shadow: 0 4px 8px -2px rgba(0, 0, 0, 0.15);
				height: 2px;
				left: unset;
			}

			&-nav {
				align-items: center;
				border-bottom: 1px solid #848e9c;
				box-shadow: 0 4px 8px -2px rgba(0, 0, 0, 0.15);

				&-list {
					.td-tabs-tab {
						margin: 0 !important;

						.td-tabs-tab-btn {
							color: #848e9c;
							font-size: 0.875em;
							padding: 0.7em 1em !important;
						}
					}

					.td-tabs-tab-active {
						.td-tabs-tab-btn {
							color: #fff;
							font-weight: 400;
						}
					}
				}
			}
		}

		.td-mobile-cpn-order-book {
			margin-bottom: 0.8em;
			margin-top: 0.4em;
		}

		div.td-markets-history-list-container {
			font-size: 0.75em;
			height: 400px;
			padding: 0;
			position: relative;

			.td-table__body {
				tr td span {
					height: 100%;
				}
			}
		}
	}
}


{/* Wrapper Tab Page Component */}

.td-mobile-cpn-wrapper-tab-page {
	margin-bottom: 1em;
	margin-top: 1em;
	padding: var(--mobile-main-padding-lr-size);
	padding-bottom: 42px;

	&__header {
		align-items: center;
		background-color: #313445;
		border-radius: 3px;
		box-shadow: 0 4px 8px -2px rgba(0, 0, 0, 0.15);
		display: flex;
		flex-direction: column;
		padding-bottom: 2em;
		padding-left: 15px;
		padding-top: 2em;

		&__h3 {
			color: #fff;
			font-size: 2.8em;
			font-weight: 500;
			line-height: 41px;
			margin-bottom: 0.3em;
			text-align: center;
		}

		&__search {
			background: rgba(196, 196, 196, 0.28);
			border: 0.5px solid #848e9c;
			border-radius: 3px;
			box-sizing: border-box;
			display: flex;
			height: 3em;
			justify-content: center;
			margin-bottom: 2em;
			position: relative;
			width: 85%;

			input {
				background: rgba(196, 196, 196, 0.28);
				border: none;
				color: #fff;
				height: 100%;
				outline: none;
				padding-left: 1em;
				width: 100%;
			}

			.icon-search {
				align-items: center;
				background: rgba(196, 196, 196, 0.28);
				display: flex;
				justify-content: center;
				position: relative;
				width: 15%;
			}

			::placeholder {
				color: rgba(196, 196, 196, 0.28);
			}
		}

		&__buttons {
			background: rgba(132, 142, 156, 0.35);
			border-radius: 3px;
			box-sizing: border-box;
			display: flex;
			width: 95%;

			&-btn {
				align-items: center;
				background-color: transparent;
				border: 1px solid rgba(196, 196, 196, 0.28);
				color: #f5f5f5;
				flex: 1;
				line-height: 20px;
				margin: 0;
				min-height: 3em;
				outline: none;
				padding: 3px 7px;
				text-align: center;
				transition: ease-in-out 0.1s;

				&:first-child {
					border-radius: 4px 0 0 4px;
				}

				&:last-child {
					border-radius: 0 4px 4px 0;
				}
			}

			&__upcoming {
				background: #e06211;
			}

			&__running {
				background: #2fb67e;
			}

			&__all {
				background: #c7c7c7;
			}
		}
	}

	&__body {
		border-radius: 10px;
		padding: 1em 0;

		&__empty {
			&__img {
				height: 9em;
				width: 9em;
			}

			h5 {
				font-size: 1.2em;
			}
		}
	}

	.pagination {
		display: flex;
		justify-content: center !important;

		.page-item {
			.page-link {
				background-color: #272b3c;
				border: none;
				border-radius: 4px;
				color: #848e9c;
				line-height: 16px;
				margin: 3px;
				min-width: 30px;
				text-align: center;

				&#active {
					background: rgba(132, 142, 156, 0.35) !important;
					color: #ffff;
				}

				&#non-active {
					background: rgba(44, 40, 40, 0.35) !important;
				}
			}
		}
	}
}



