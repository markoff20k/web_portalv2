{/*   *** CUSTOM INPUT COMPONENT *** */}

.mobile-custom-input {
	padding: '1rem 0';

	input {
		border-color: var(--input-border-color);
		font-size: 12px !important;
	}

	label {
		//background: var(--mobile-body-background-color) !important;
		background: transparent !important;
		color: #f5f5f5;
		font-size: 12px;
		left: 2%;
		margin-top: -8px;
		padding: 0 calc(var(--gap) * 0.7);
		position: absolute;
		top: 0;
		z-index: 5;
	}

	input,
	input:focus {
		background-color: #009991 !important;
		box-shadow: none;
		color: #f5f5f5 !important;
	}

	input:focus-within {
		border-color: #009991;

		label {
			//background-color: var(--mobile-body-background-color) !important;
			background-color: transparent!important;
			color: #f5f5f5;
		}
	}

	input:-webkit-autofill,
	input:-webkit-autofill:hover,
	input:-webkit-autofill:focus {
		border: 1px solid #009991;
		-webkit-box-shadow: 0 0 0 1000px #009991 inset;
		-webkit-text-fill-color: var(--input-text-color);
		transition: background-color 5000s ease-in-out 0s;
	}
}

.pg-mobile-app .custom-input {
	font-size: 12px;

	input {
		font-size: 12px;

		&::placeholder {
			font-size: 12px;
		}
	}
}


{/* Order Input V2 */}
.cr-order-input-mobile {
	align-items: center;
